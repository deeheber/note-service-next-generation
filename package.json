{
  "name": "note-service-next-generation",
  "version": "2.0.0",
  "bin": {
    "note-service": "bin/note-service.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run clean && npm run build && npm run cdk deploy -a NoteServiceStack",
    "destroy": "npm run cdk destroy -a NoteServiceStack",
    "synth": "npm run cdk synth > template.yaml"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.8.0",
    "constructs": "^10.0.36"
  },
  "devDependencies": {
    "aws-cdk": "2.27.0",
    "@aws-cdk/assert": "2.27.0",
    "@aws-cdk/aws-appsync-alpha": "2.27.0-alpha.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "aws-cdk-lib": "2.27.0",
    "constructs": "^10.0.109",
    "esbuild": "^0.14.34",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.109.0",
    "@aws-sdk/lib-dynamodb": "^3.109.0",
    "@aws-sdk/util-dynamodb": "^3.109.0"
  }
}
