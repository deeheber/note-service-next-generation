{
  "name": "note-service-next-generation",
  "version": "2.0.0",
  "bin": {
    "note-service": "bin/note-service.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run clean && npm run build && npm run cdk deploy -a NoteServiceStack",
    "destroy": "npm run cdk destroy -a NoteServiceStack",
    "synth": "npm run cdk synth > template.yaml"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.34.2",
    "constructs": "^10.1.62"
  },
  "devDependencies": {
    "aws-cdk": "2.34.2",
    "@aws-cdk/assert": "2.34.2",
    "@aws-cdk/aws-appsync-alpha": "2.34.2-alpha.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "aws-cdk-lib": "2.34.2",
    "constructs": "^10.1.62",
    "esbuild": "^0.14.51",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.137.0",
    "@aws-sdk/lib-dynamodb": "^3.137.0",
    "@aws-sdk/util-dynamodb": "^3.137.0"
  }
}
