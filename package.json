{
  "name": "note-service-next-generation",
  "version": "2.0.0",
  "bin": {
    "note-service": "bin/note-service.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run clean && npm run build && npm run cdk deploy -a NoteServiceStack",
    "destroy": "npm run cdk destroy -a NoteServiceStack",
    "synth": "npm run cdk synth > template.yaml"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.84.0",
    "constructs": "10.2.52"
  },
  "devDependencies": {
    "@aws-cdk/assertions": "^1.203.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.18",
    "aws-cdk": "2.84.0",
    "aws-cdk-lib": "2.84.0",
    "constructs": "10.2.52",
    "esbuild": "^0.18.4",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.354.0",
    "@aws-sdk/lib-dynamodb": "^3.354.0",
    "@aws-sdk/util-dynamodb": "^3.354.0"
  }
}
